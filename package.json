{
  "name": "test-zoneless",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p start:*",
    "start:zone": "ng serve test-zoneless_zone --port 4201",
    "start:zoneless": "ng serve test-zoneless_zoneless --port 4202",
    "build": "npm-run-all -s build:all tool:move_file tool:replace_index.html",
    "build:all": "npm-run-all -p build:zone build:zoneless",
    "build:zone": "ng build test-zoneless_zone",
    "build:zoneless": "ng build test-zoneless_zoneless",
    "build:--base-href:zone": "ng build --base-href /test-zoneless/ test-zoneless_zone",
    "build:--base-href:zoneless": "ng build --base-href /test-zoneless/ test-zoneless_zoneless",
    "watch": "npm-run-all -p watch:*",
    "watch:zone": "ng build test-zoneless_zone --watch",
    "watch:zoneless": "ng build test-zoneless_zoneless --watch",
    "test": "ng test",
    "fmt": "prettier --write src tool",
    "tool:move_file": "tsx tool/copy_file.ts",
    "tool:replace_index.html": "tsx tool/replace_index.html.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.1",
    "@angular/cli": "^18.2.1",
    "@angular/compiler-cli": "^18.2.0",
    "@types/jasmine": "~5.1.0",
    "execa": "^9.5.1",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^2.1.0",
    "prettier": "^3.3.3",
    "tsx": "^4.16.5",
    "typescript": "~5.5.2"
  }
}
